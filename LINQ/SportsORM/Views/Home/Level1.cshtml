    </div>
    <div class="card-header bg-dark text-light">First Name Alexander or Wyatt</div>
    <div class="card-body">
        <ul>
            @{
                foreach (Player p in ViewBag.allAlexOrWyatt)
                {
                    <li>@p.FirstName @p.LastName</li>
                }
            }
        </ul>
    </div>
</div>