                }
            }
        </ul>
    </div>
    <div class="card-header bg-dark text-light">Last Name Cooper</div>
    <div class="card-body">
        <ul>
            @{
                foreach (Player p in ViewBag.lastNameCoop)
                {
                    <li>@p.FirstName @p.LastName</li>
                }
            }
        </ul>
    </div>
    <div class="card-header bg-dark text-light">First Name Joshua</div>
