    </div>
    <div class="card-header bg-dark text-light">Ordered by Location</div>
    <div class="card-body">
        <ul>
            @{
                foreach (Team t in ViewBag.orderedByLocation)
                {
                    <li>@t.Location @t.TeamName</li> 
                }
            }
        </ul>
    </div>
    <div class="card-header bg-dark text-light">Ordered by Team Name in Reverse</div>
